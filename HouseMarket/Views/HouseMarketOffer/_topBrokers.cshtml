@model HouseMarket.Models.HouseMarketOffer.TopBrokerListViewModel


<table class="table">
    <thead>
    <tr>
        <th>Broker Id</th>
        <th>Broker Name</th>
        <th>Number of Houses</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var broker in @Model.TopBrokers)
    {
        <tr>
            <td>@broker.Id</td>
            <td>@broker.Name</td>
            <td>@broker.NumberOfHouses</td>
        </tr>
    }
    </tbody>
</table>


